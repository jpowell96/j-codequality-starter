<!-- This file is used to exclude certain classes or errors from being checked by SpotBugs.
Documentation  can be found at https://spotbugs.readthedocs.io/en/latest/filter.html
-->
  <?xml version="1.0" encoding="UTF-8"?>
  <FindBugsFilter
              xmlns="https://github.com/spotbugs/filter/4.8.4"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="https://github.com/spotbugs/filter/4.8.4 https://raw.githubusercontent.com/spotbugs/spotbugs/4.8.4/spotbugs/etc/findbugsfilter.xsd">

  <!-- Ignore naming convention problems -->
    <Match>
        <Bug code="NM_METHOD_NAMING_CONVENTION" />
    </Match>
</FindBugsFilter>